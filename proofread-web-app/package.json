    "dependencies": {
        "@chakra-ui/next-js": "^2.2.0",
        "@chakra-ui/react": "^2.8.2",
        "@emotion/react": "^11.11.4",
        "axios": "^1.4.0",
        "framer-motion": "^11.0.8",
        "langchain": "^0.1.25",
        "next": "14.1.1",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@emotion/styled": "^11.11.0",
        "@types/node": "20.2.5",
        "@types/react": "18.2.8",
        "@types/react-dom": "18.2.4",
        "eslint": "^8",
        "eslint-config-next": "14.1.1",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3"
    }
}
