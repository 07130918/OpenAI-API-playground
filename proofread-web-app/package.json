{
    "name": "proofread",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint src --ext .js,jsx,.ts,.tsx --fix",
        "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'"
    },
    "prettier": {
        "trailingComma": "all",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "printWidth": 100
